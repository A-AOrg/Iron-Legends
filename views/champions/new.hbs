<h2>Add New Champion</h2>

<form method="POST" action="/champions">
  <div>
    <label>Name</label>
    <input name="name" type="text" placeholder="Name"/>
  </div>

  <div>
    <label>Title</label>
    <input name="title" type="text" placeholder="Title"/>
  </div>

  <div>
    <label>Key</label>
    <input name="key" type="number" placeholder="Key"/>
  </div>

   <div>
    <label>Splashart</label>
    <input name="splashart" type="text" placeholder="Splashart"/>
  </div>

   <div>
    <label>Icon</label>
    <input name="icon" type="text" placeholder="Icon"/>
  </div>

   <div>
    <label>Frame</label>
    <input name="frame" type="text" placeholder="Frame"/>
  </div>

  <button type="submit">Create</button>

</form>